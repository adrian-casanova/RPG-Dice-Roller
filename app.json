{
  "expo": {
    "name": "dnd",
    "slug": "dnd",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "android": {
      "package": "com.accmobiletech.dndDiceRoller"
    },
    "ios": {
      "bundleIdentifier": "com.accmobiletech.dndDiceRoller",
      "supportsTablet": true
    },
    "assetBundlePatterns": ["**/*"]
  }
}
